/* Global Stylesheet */
html, body { @apply h-full overflow-hidden; }

@font-face {
  /* Reference name */
  font-family: 'Inter';
  /* URL to font file */
  src: url('/fonts/Inter-VariableFont_slnt,wght.ttf');
}

:root {
  --theme-font-family-base: 'Inter', sans-serif;
  --theme-font-family-heading: 'Inter', sans-serif;
}
